<!DOCTYPE ejs>
<html>
  <head>
    <title>Sportify || Main Page</title>

    <link rel="stylesheet" type="text/css" href="../css/main_page.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdn.socket.io/socket.io-3.0.1.min.js"></script>
    <script src="https://d3js.org/d3.v6.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.7.0.min.js" integrity="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g=" crossorigin="anonymous"></script>  


  </head>
  <body>
    <%- include('./templates/header.ejs');%>
    <div class="welcome-header">
      <h2>WELCOME TO SPORTIFY</h2>
    </div>
    <%- include('./templates/dropDown.ejs');%>

    <% if (showFire) { %>                  
      <script>
        async function PromocodeMessage(){
    await Swal.fire({
        title: 'Promocode ',
        text: "Your promocode for first buy is: 'welcome'",
        icon: 'success',
        confirmButtonText: 'OK'
      });
    }
    PromocodeMessage();
      </script>
    <% } %>
    <% if(!loggedIn) { %>     
    <div class="discount10">  
           <a href="/register" class="discount-link">  
                  <h3>Not a member?  recieve 10% discount at sign-up</h3>  
                  </a>
                        
              </div>
              <br>
              <% } %>
              <br>
              <div class="UsersOnline">
              <span  id="UsersCounnt">There is 0 Users online</span>
            </div>
            <br>
              <div class="container mt-4">
                <div id="carouselExample" class="carousel slide" data-bs-ride="carousel">
                    <div class="carousel-indicators">
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="1" aria-label="Slide 2"></button>
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="2" aria-label="Slide 3"></button>
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="3" aria-label="Slide 4"></button>
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="4" aria-label="Slide 5"></button>
                        <button type="button" data-bs-target="#hero-carousel" data-bs-slide-to="5" aria-label="Slide 6"></button>
                      </div>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img src="../ImagesFront/1.png" class="d-block w-100" alt="Photo 1">
                        </div>
                        <div class="carousel-item">
                            <img src="../ImagesFront/2.png" class="d-block w-100" alt="Photo 2">
                        </div>
                        <div class="carousel-item">
                            <img src="../ImagesFront/3.png" class="d-block w-100" alt="Photo 3">
                        </div>
                        <div class="carousel-item">
                            <img src="../ImagesFront/4.png" class="d-block w-100" alt="Photo 4">
                        </div>
                        <div class="carousel-item">
                            <img src="../ImagesFront/5.png" class="d-block w-100" alt="Photo 5">
                        </div>
                        <div class="carousel-item">
                            <img src="../ImagesFront/6.png" class="d-block w-100" alt="Photo 6">
                        </div>
                    </div>
                    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Previous</span>
                    </button>
                    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="visually-hidden">Next</span>
                    </button>
                </div>
            </div>

            <div class="chart-container">
              <svg id="orderChart"></svg>
              <svg id="promoChart"></svg>
          </div>
          <div class="chart2-container">
            <svg id="GenderChart"></svg>
